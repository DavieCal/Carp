@article{stuart_2006,
  title = {Movement of common carp, {{Cyprinus}} carpio, in a regulated lowland {{Australian}} river: implications for management},
  volume = {13},
  issn = {1365-2400},
  shorttitle = {Movement of common carp, {{Cyprinus}} carpio, in a regulated lowland {{Australian}} river},
  doi = {10.1111/j.1365-2400.2006.00495.x},
  abstract = {Abstract\hspace{0.6em} Introduced common carp, Cyprinus carpio L., is the most abundant large fish in the Murray-Darling Basin and past studies have considered it non-migratory. Movement information is, however, important for targeting control efforts to appropriate habitats and spatial scales. Movement patterns of common carp in the Murray River were investigated between November 1999 and August 2001. In total, 3337 common carp were boat electric fished and externally tagged. Of the 293 (8.8\%) fish recaptured, up to 1898~days later, the majority (80\%) moved \ensuremath{<}5~km. However, over 7\% of the tagged population moved \ensuremath{\geq}100~km, and the maximum distance was 890~km. Intermediate-sized males moved the greatest distances, usually downstream. Conversely, females and larger males remained close to floodplain spawning areas. For improved management, there is a need to recognise different movement strategies within common carp populations.},
  language = {en},
  timestamp = {2016-01-18T23:05:32Z},
  number = {4},
  urldate = {2016-01-18},
  journal = {Fisheries Management and Ecology},
  author = {{Stuart}, I. G. and {Jones}, M. J.},
  month = aug,
  year = {2006},
  keywords = {Common carp,control,floodplain,invasive,Murray River,tagging},
  pages = {213--219},
  file = {Full Text PDF:/Users/DavieCal/Library/Application Support/Zotero/Profiles/m82ety9a.default/zotero/storage/DV2XQZSU/Stuart and Jones - 2006 - Movement of common carp, Cyprinus carpio, in a reg.pdf:application/pdf;Snapshot:/Users/DavieCal/Library/Application Support/Zotero/Profiles/m82ety9a.default/zotero/storage/MSCHTFC8/abstract.html:}
}

